{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
	"baseUrl": "./src",
	"paths": {
		"@assets/*": ["assets/*"],
		"@api/*": ["api/*"],
		"@components/*": ["components/*"],
		"@constants/*": ["constants/*"],
		"@contexts/*": ["contexts/*"],
		"@hooks/*": ["hooks/*"],
		"@navigation/*": ["navigation/*"],
		"@screens/*": ["screens/*"],
		"@styles/*": ["styles/*"],
		"@types/*": ["types/*"],
		"@utils/*": ["utils/*"]

		// Make sure these match your Babel aliases
	  }
  }
}
